pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
-- koku rnd
-- blackspotbear 2016

koku=25 -- rnd = (rnd1 + rnd2 + ...) / koku
smax=2 -- max speed
amax=0.25 -- max angle
bmax=500 -- max num of bullet
bs={} -- bullets

-- angle of fire
function arnd(n)
	a=0
	for i=1,n do
		a+=rnd(amax)-amax/2
	end
	return a/n
end

-- bullet speed
function srnd(n)
	s=0
	for i=1,n do
		s+=rnd(smax)
	end
	return s/n+1
end

function shot(d)
	b={}
	if d==1 then
		a=arnd(koku)
		s=srnd(koku)
	else
		a=arnd(1)
		s=srnd(1)
	end
	b.x=64;b.y=64
	b.vx=s*cos(a)
	b.vy=s*sin(a)
	b.vx*=d
	return b
end

function _update()
	-- shot bullets
	d=1 -- bullet direction (right or left)
	if #bs==0 then
		for i=1,bmax do
			bs[#bs+1]=shot(d)
			d*=-1
		end
	end

	bt={}
	for i=1,#bs do
		-- move bullets
		bs[i].x+=bs[i].vx
		bs[i].y+=bs[i].vy
		-- delete bullet if out of screen
		if bs[i].x>=0 and bs[i].x<128 and bs[i].y>=0 and bs[i].y<128 then
			bt[#bt+1]=bs[i]
		end
	end
	bs=bt
end

function _draw()
	cls()

	-- draw emitter
	circfill(64,64,1,10)
	for i=1,#bs do
		-- draw particles
		pset(bs[i].x, bs[i].y, 14)
	end
end
